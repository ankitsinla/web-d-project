	<%- include("../partials/header") %>

<div class="jumbotron " style="text-align:center">
		<h1><%=currentUser.username%></h1>
	<a href="/admin"><button class="btn btn-secondary">Home</button></a>	
</div>

<div class="container">
	 <div class="row"  style="margin:auto;">
		<div class="col-lg-12">
			<h1 class="text-center display-4 my-4">Professors Initial Credientials</h1>
		
	<table class="table" style="width:80%;margin:auto;text-align:center">
		<hr>
		<thead class="thead-dark">
			<tr>
				<th>Name</th>
				<th>College Id</th>
				<th>Password</th>
				<th>Is SignedUp</th>
			</tr>
		</thead>
		<tbody>
			<% profs.forEach(function(prof){ %>
				<tr>
					<td><%= prof.name %></td>
					<td><%= prof.collegeId %></td>
					<%list.forEach(function(data){%>
						<%if(data.id.equals(prof._id)){ %>
							<td><%=data.password %></td>
							<%if(data.isRegister == "false"){%>
								<td>NO</td>
							<%}else{%>
								<td>YES</td>
							<%}%>
						<%}%>
					<%})%>
				</tr>
			<%})%>			
		</tbody>
	</table>
			<hr style="height:2px;border-width:0;color:gray;background-color:gray">	
		 </div>
	</div>
	
</div>

<%- include("../partials/footer") %>